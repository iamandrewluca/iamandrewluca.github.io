{
	"name": "iamandrewluca.github.io",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"node": ">= 18.0.0",
		"npm": ">= 9.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && tsc --noEmit && astro build",
		"preview": "astro preview",
