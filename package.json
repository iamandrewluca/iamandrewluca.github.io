		"postinstall": "patch-package; npx tsx scripts/extract-styles.ts; npx tsx scripts/stats-fetch.ts"
	},
	"dependencies": {
		"@allindevelopers/prettier-config": "^1.0.0",
		"@astrojs/check": "^0.3.2",
		"@astrojs/react": "^3.0.7",
		"@astrojs/tailwind": "^5.0.3",
		"@commitlint/cli": "^18.4.3",
		"@commitlint/config-conventional": "^18.4.3",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"astro": "^4.0.4",
		"cheerio": "1.0.0-rc.12",
		"husky": "^8.0.3",
		"lint-staged": "^15.2.0",
		"patch-package": "^8.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-astro": "^0.12.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"react": "^18.2.0",
		"react-activity-calendar": "^2.2.0",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.3.6",
		"tsx": "^4.6.2",
		"typescript": "^5.3.3"
	},
	"lint-staged": {
		"*": "npx prettier --write --ignore-path .gitignore --ignore-unknown"
