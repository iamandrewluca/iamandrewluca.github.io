{
	"name": "iamandrewluca.github.io",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"node": ">= 18.0.0",
		"npm": ">= 9.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && tsc --noEmit && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"prepare": "husky",
		"postinstall": "patch-package; npx tsx scripts/extract-styles.ts; npx tsx scripts/stats-fetch.ts"
	},
	"dependencies": {
		"@allindevelopers/prettier-config": "^1.0.0",
		"@astrojs/check": "^0.7.0",
		"@astrojs/react": "^3.4.0",
		"@astrojs/tailwind": "^5.1.0",
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"astro": "^4.4.13",
		"astro-critters": "^2.1.9",
		"cheerio": "1.0.0-rc.12",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.5",
		"patch-package": "^8.0.0",
		"prettier": "^3.2.5",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-tailwindcss": "^0.5.14",
		"react": "^18.3.1",
		"react-activity-calendar": "^2.2.8",
		"react-dom": "^18.3.1",
		"tailwindcss": "^3.4.3",
		"tailwindcss-animated": "^1.0.1",
		"tsx": "^4.11.0",
		"typescript": "^5.4.5"
	},
	"lint-staged": {
		"scripts/contributions/*.json": "npx tsx scripts/l10n-sort.ts",
		"*": "npx prettier --write --ignore-path .gitignore --ignore-unknown"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
