{
	"name": "iamandrewluca.github.io",
	"type": "module",
	"version": "0.0.1",
	"engines": {
		"node": ">= 18.0.0",
		"npm": ">= 9.0.0"
	},
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro check && tsc --noEmit && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"prepare": "husky install",
		"postinstall": "patch-package; npx tsx scripts/extract-styles.ts; npx tsx scripts/stats-fetch.ts"
	},
	"dependencies": {
		"@allindevelopers/prettier-config": "^1.0.0",
		"@astrojs/react": "^2.2.1",
		"@astrojs/tailwind": "^4.0.0",
		"@commitlint/cli": "^17.6.6",
		"@commitlint/config-conventional": "^17.6.6",
		"@types/react": "^18.2.14",
		"@types/react-dom": "^18.2.6",
		"astro": "^2.7.0",
		"cheerio": "^1.0.0-rc.12",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.2",
		"patch-package": "^7.0.0",
		"prettier": "^2.8.8",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"react": "^18.2.0",
		"react-activity-calendar": "^2.0.2",
		"react-dom": "^18.2.0",
		"tailwindcss": "^3.3.2",
		"tsx": "^3.12.7"
	},
	"lint-staged": {
		"*": "npx prettier --write --ignore-path .gitignore --ignore-unknown"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	}
}
